{
  "$schema": "https://raw.githubusercontent.com/nuke-build/nuke/master/source/Nuke.CodeGeneration/schema.json",
  "name": "AppCenter",
  "officialUrl": "https://github.com/Microsoft/appcenter-cli/",
  "help": "Nuke Tool for AppCenter CLI",
  "pathExecutable": "appcenter",
  "tasks": [
    {
      "help": "Log in",
      "postfix": "Login",
      "definiteArgument": "login",
      "settingsClass": {
        "properties": [
          {
            "name": "User",
            "type": "string",
            "format": "--user {value}",
            "help": "Username to log in as"
          },
          {
            "name": "Password",
            "type": "string",
            "format": "--password {value}",
            "secret": true,
            "help": "Password to log in with"
          }
        ]
      }
    },
    {
      "help": "Log out",
      "postfix": "Logout",
      "definiteArgument": "logout",
      "settingsClass": {}
    },
    {
      "help": "Create a new app",
      "postfix": "AppsCreate",
      "definiteArgument": "apps create",
      "settingsClass": {
        "properties": [
          {
            "name": "Platform",
            "type": "AppCenterAppsCreatePlatform",
            "format": "--platform {value}",
            "help": "The platform of the app. Supported values: Cordova, Java, Objective-C-Swift, React-Native, Unity, UWP, WinForms, WPF, Xamarin, Custom"
          },
          {
            "name": "OS",
            "type": "AppCenterAppsCreateOS",
            "format": "--os {value}",
            "help": "The OS the app will be running on. Supported values: Android, Custom, iOS, macOS, tvOS, Windows"
          },
          {
            "name": "DisplayName",
            "type": "string",
            "format": "--display-name {value}",
            "help": "The descriptive name of the app. This can contain any characters"
          },
          {
            "name": "ReleaseType",
            "type": "AppCenterAppsCreateReleaseType",
            "format": "--release-type {value}",
            "help": "The app release type. Suggested values are Alpha, Beta, Production, Store, Enterprise. Custom values are allowed and must be must be one word, alphanumeric, first letter capitalized."
          },
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}",
            "help": "The name of the app used in URLs. Can optionally be provided specifically, otherwise a generated name will be derived from display-name"
          },
          {
            "name": "Description",
            "type": "string",
            "format": "--description {value}",
            "help": "The description of the app"
          }
        ]
      }
    },
    {
      "help": "Upload release binary and trigger distribution, at least one of --store or --group must be specified",
      "postfix": "DistributeRelease",
      "definiteArgument": "distribute release",
      "settingsClass": {
        "properties": [
          {
            "name": "File",
            "type": "string",
            "format": "--file {value}",
            "help": "Path to binary file"
          },
          {
            "name": "Timeout",
            "type": "int",
            "format": "--timeout {value}",
            "help": "Timeout for waiting release id (in seconds)"
          },
          {
            "name": "Mandatory",
            "type": "bool",
            "format": "--mandatory",
            "help": "Make the release mandatory for the testers (default is false)"
          },
          {
            "name": "Silent",
            "type": "bool",
            "format": "--silent",
            "help": "Do not notify testers of this release"
          },
          {
            "name": "ReleaseNotesFile",
            "type": "string",
            "format": "--release-notes-file {value}",
            "help": "Path to release notes file (markdown supported, 5000 characters max)"
          },
          {
            "name": "ReleaseNotes",
            "type": "string",
            "format": "--release-notes {value}",
            "help": "Release notes text (5000 characters max)"
          },
          {
            "name": "Store",
            "type": "string",
            "format": "--store {value}",
            "help": "Store name"
          },
          {
            "name": "Group",
            "type": "List<string>",
            "format": "--group {value}",
            "separator": ",",
            "help": "Comma-separated distribution group names"
          },
          {
            "name": "BuildNumber",
            "type": "string",
            "format": "--build-number {value}",
            "help": "Build number parameter required for macOS .pkg and .dmg files"
          },
          {
            "name": "BuildVersion",
            "type": "string",
            "format": "--build-version {value}",
            "help": "Build version parameter required for .zip, .msi, .pkg and .dmg files"
          },
          {
            "name": "App",
            "type": "string",
            "format": "--app {value}",
            "help": "Specify app in the &lt;ownerName&gt;/&lt;appName&gt; format"
          }
        ]
      }
    }
  ],
  "commonTaskProperties": [
    {
      "name": "DisableTelemetry",
      "type": "bool",
      "format": "--disable-telemetry",
      "help": "Disable telemetry for this command"
    },
    {
      "name": "Version",
      "type": "bool",
      "format": "--version",
      "help": "Display AppCenter version"
    },
    {
      "name": "Quiet",
      "type": "bool",
      "format": "--quiet",
      "help": "Auto-confirm any prompts without waiting for input"
    },
    {
      "name": "Help",
      "type": "bool",
      "format": "--help",
      "help": "Display help for current command"
    },
    {
      "name": "Env",
      "type": "string",
      "format": "--env {value}",
      "help": "Environment when using API token"
    },
    {
      "name": "Token",
      "type": "string",
      "format": "--token {value}",
      "secret": true,
      "help": "API token"
    },
    {
      "name": "Output",
      "type": "AppCenterDistributeReleaseOutput",
      "format": "--output {value}",
      "help": "Output format: json"
    },
    {
      "name": "Debug",
      "type": "bool",
      "format": "--debug",
      "help": "Display extra output for debugging"
    }
  ],
  "enumerations": [
    {
      "name": "AppCenterAppsCreatePlatform",
      "values": [
        "Cordova",
        "Java",
        "Objective-C-Swift",
        "React-Native",
        "Unity",
        "UWP",
        "Xamarin",
        "Custom"
      ]
    },
    {
      "name": "AppCenterAppsCreateOS",
      "values": [
        "Android",
        "Custom",
        "iOS",
        "macOS",
        "tvOS",
        "Windows"
      ]
    },
    {
      "name": "AppCenterAppsCreateReleaseType",
      "values": [
        "Alpha",
        "Beta",
        "Production",
        "Store",
        "Enterprise"
      ]
    },
    {
      "name": "AppCenterDistributeReleaseOutput",
      "values": [
        "json"
      ]
    }
  ],
  "_metadata": {
    "version": "2.11.0"
  }
}